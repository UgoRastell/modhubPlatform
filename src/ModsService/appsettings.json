{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "https://modhub.ovh",
  "MongoDbSettings": {
    "ConnectionString": "mongodb://root:example@mongodb:27017",
    "DatabaseName": "modsdb"
  },
  "JwtSettings": {
    "Key": "YourSecretKeyHere",
    "Issuer": "ModHub",
    "Audience": "ModHubUsers"
  },
  "RabbitMQ": {
    "Host": "rabbitmq",
    "Port": 5672,
    "Username": "guest",
    "Password": "guest"
  },
  "ConnectionStrings": {
    "Redis": "redis:6379"
  },
  "GrpcServices": {
    "FileService": "http://fileservice:8080"
  },
  "FileStorage": {
    "UploadsBasePath": "/var/www/modhub/uploads",
    "ModsRelativePath": "mods",
    "ImagesRelativePath": "images"
  }
}
